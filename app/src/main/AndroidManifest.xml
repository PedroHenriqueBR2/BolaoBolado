<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pedro.bolaobolado">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoaplicativo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Adicionar"
            android:label="Nova Aposta" />

        <activity
            android:name=".InformacaoAposta"
            android:label="Informações da aposta" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".InformacaoUpdate"
            android:label="Atualizar dados"></activity>
    </application>

</manifest>